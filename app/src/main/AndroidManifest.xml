    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pixels">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation = "portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondActivity"
            android:screenOrientation = "portrait"/>
        <activity android:name=".SettingActivity" android:screenOrientation = "portrait"/>
        <activity android:name="NewsActivity"  android:screenOrientation = "portrait"/>
        <activity android:name="EducActivity"  android:screenOrientation = "portrait"/>
        <activity android:name="CollActivity"  android:screenOrientation = "portrait"/>
        <activity android:name=".SettingActivitySaved"  android:screenOrientation = "portrait"/>
    </application>

</manifest>